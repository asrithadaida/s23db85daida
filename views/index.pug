extends layout

block content
  h1= title
  p My hobbies
  button(type="button", class="btn btn-primary" onClick="goFaster()") PressMe
  div(id="faster") Increase the speed
  a(href="/Vehicle") Vehicle
  script. 
      let mph = 20;
      let change = 1;
      function goFaster(){
        mph += change;
        change += 1;
        document.getElementById("faster").innerHTML = "MPH "+mph+" with change "+change;
      }